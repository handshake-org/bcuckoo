{
  "name": "bcuckoo",
  "version": "0.3.2",
  "description": "Cuckoo Cycle verification and miner in pure javascript",
  "keywords": [
    "blake2b",
    "cuckoo",
    "cycle",
    "pow",
    "siphash"
  ],
  "license": "MIT",
  "repository": "git://github.com/bcoin-org/bcuckoo.git",
  "homepage": "https://github.com/bcoin-org/bcuckoo",
  "bugs": {
    "url": "https://github.com/bcoin-org/bcuckoo/issues"
  },
  "author": "Christopher Jeffrey <chjjeffrey@gmail.com>",
  "main": "./lib/bcuckoo.js",
  "scripts": {
    "browserify": "browserify -s bcuckoo lib/bcuckoo.js | uglifyjs -c > bcuckoo.js",
    "clean": "rm -f bcuckoo.js",
    "lint": "eslint lib/ test/ || exit 0",
    "test": "mocha --reporter spec test/*-test.js",
    "webpack": "webpack --mode production --config webpack.config.js"
