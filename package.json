    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "browserify": "^14.5.0",
    "eslint": "^4.14.0",
    "mocha": "^4.0.1",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "uglify-es": "^3.1.3",
    "webpack": "^3.10.0"
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "browserify": {
    "transform": [
